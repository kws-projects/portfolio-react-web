{
  "name": "portfolio-react-web",
  "private": true,
  "version": "1.2.0",
  "type": "module",
  "scripts": {
    "dev": "cp .env.development.local .env && vite",
    "prod": "cp .env.production.local .env && vite",
    "build:dev": "cp .env.development.local .env && vite build",
    "build:prod": "cp .env.production.local .env && vite build && yarn sentry:sourcemaps",
    "lint": "eslint .",
    "prettier": "npx prettier . --write",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "upload-source-maps": "npx @sentry/wizard@latest -i sourcemaps",
    "sentry:sourcemaps": "sentry-cli sourcemaps inject --org kenneth-wong --project portfolio-react-web ./dist && sentry-cli sourcemaps upload --org kenneth-wong --project portfolio-react-web ./dist",
    "prepare": "husky",
    "pre-commit": "lint-staged",
    "pre-push": "tsc",
    "preview": "vite preview"
  },
  "dependencies": {
    "@monaco-editor/react": "^4.6.0",
    "@sentry/cli": "^2.39.1",
    "@sentry/react": "^8.42.0",
    "@tanstack/react-query": "^5.62.3",
    "axios": "^1.7.9",
    "dayjs": "^1.11.13",
    "dompurify": "^3.2.2",
    "enum-values": "^1.2.1",
    "framer-motion": "^11.13.1",
    "p5": "^1.11.2",
    "qs": "^6.13.1",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "react-ga4": "^2.1.0",
    "react-helmet-async": "^2.0.5",
    "react-icons": "^5.4.0",
    "react-markdown": "^9.0.1",
    "react-router-dom": "^7.0.2",
    "remark-gfm": "^4.0.0"
  },
  "devDependencies": {
    "@commitlint/cli": "^19.6.0",
    "@commitlint/config-conventional": "^19.6.0",
    "@eslint/js": "^9.15.0",
    "@types/dompurify": "^3.2.0",
    "@types/node": "^22.10.1",
    "@types/p5": "^1.7.6",
    "@types/qs": "^6.9.17",
    "@types/react": "^19.0.1",
    "@types/react-dom": "^19.0.1",
    "@vitejs/plugin-react": "^4.3.4",
    "@vitejs/plugin-react-swc": "^3.7.2",
    "autoprefixer": "^10.4.20",
    "eslint": "^9.16.0",
    "eslint-plugin-react-hooks": "^5.0.0",
    "eslint-plugin-react-refresh": "^0.4.14",
    "globals": "^15.12.0",
    "husky": "^9.1.7",
    "i18next": "^24.0.5",
    "lint-staged": "^15.2.10",
    "postcss": "^8.4.49",
    "prettier": "^3.4.2",
    "react-i18next": "^15.1.3",
    "tailwindcss": "^3.4.16",
    "typescript": "^5.7.2",
    "typescript-eslint": "^8.15.0",
    "vite": "^6.0.1"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  }
}
